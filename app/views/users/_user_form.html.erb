<%= form_for @user, html: {method: :put} do |f| %>
	<%= f.label :fname, "First Name"%>
	<%= f.text_field :fname %>
	<%= f.label :lname, "Last Name"%>
	<%= f.text_field :lname %>
	<%= f.label :username, "Username"%>
	<%= f.text_field :username %>
	<%= f.label :password, "Password"%>
	<%= f.password_field :password %>
	<%= f.submit %>
<% end %>